{
  "name": "ghostwriter-service",
  "version": "1.0.31",
  "description": "Prerenders your JavaScript website for search engines, SEO tools, social media crawler and your browser.",
  "bin": {
    "ghostwriter-service": "build/main.js"
  },
  "scripts": {
    "build": "babel src --out-dir build --source-maps --copy-files",
    "build-watch": "babel src --out-dir build --source-maps --copy-files --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node build/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/core-process/ghostwriter.git"
  },
  "author": "Niklas Salmoukas <niklas@salmoukas.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/core-process/ghostwriter/issues"
  },
  "homepage": "https://github.com/core-process/ghostwriter#readme",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-0": "^6.5.0"
  },
  "dependencies": {
    "argparse": "^1.0.9",
    "babel-runtime": "^6.18.0",
    "base-64": "^0.1.0",
    "body-parser": "^1.15.2",
    "deep-extend": "^0.4.1",
    "express": "^4.14.0",
    "ghostwriter-common": "^1.0.0",
    "jsonschema": "^1.1.0",
    "mongodb": "^2.2.12",
    "morgan": "^1.7.0",
    "phantomjs-prebuilt": "^2.1.13",
    "request": "^2.79.0",
    "request-promise": "^4.1.1",
    "tmp": "0.0.31",
    "underscore": "^1.8.3",
    "urijs": "^1.18.3",
    "xml2js": "^0.4.17"
  }
